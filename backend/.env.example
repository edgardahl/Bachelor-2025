# General
PORT=port
FRONTEND_PORT=another_port

# Database Selection (Use 'supabase' instead of 'postgres' for clarity)
DB_TYPE=supabase

# PostgreSQL Connection (via Supabase)
POSTGRES_URI=postgres://postgres:db.user.supabase.co:port/postgres

# Supabase Credentials (replace with actual values from Supabase dashboard)
SUPABASE_URL=https://secret_code.supabase.co
SUPABASE_KEY=secret_supabase_key

# Security
NODE_ENV=development_or_production
JWT_SECRET=our_secret_key
JWT_REFRESH_SECRET=our_secret_refresh_key
